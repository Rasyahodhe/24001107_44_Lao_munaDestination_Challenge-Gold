<h1><%=header%></h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Facilitas</th>
      <th scope="col">About</th>
      <th scope="col">Button</th>
    </tr>
  </thead>
  <tbody>
    <% destinations.forEach(des => { %> <% let i = 0 %>
    <tr>
      <th scope="row"><%= des.idDes%></th>
      <td><%= des.namaDes %></td>
      <td></td>
      <td><%=des.description%></td>
      <td>
        <div class="btn_btn">
          <a href="/ad/dash/destinations/updata">Update</a>
          <a href="/ad/dash/destinations/detail/<%= des.namaDes%>">Detail</a>
          <form action="/ad/dash/destinations/del" method="delete">
            <button type="submit" name="submit">Delete</button>
          </form>
        </div>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
